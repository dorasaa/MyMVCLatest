@model IEnumerable<MyMVCLatest.Models.Department>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DeptList</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                 
                 <td>@Html.ActionLink(item.name, "All", new { DepartmentId = item.id })</td>
            </td>
            <td>
             @*   @Html.ActionLink("Edit", "Edit", new { id=item.id }) |*@
              
                @Html.ActionLink("Delete", "DeleteDept", new { DepartmentId = item.id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
